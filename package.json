{
  "name": "maml",
  "version": "0.6.0",
  "description": "Static website generator taking markdown and yaml as input",
  "keywords": [
    "markdown",
    "yaml",
    "website"
  ],
  "author": "Tyler Long <tyler4long@gmail.com>",
  "license": "MIT",
  "bin": {
    "maml": "src/maml.js"
  },
  "scripts": {
    "upgrade": "yarn-upgrade-all",
    "test:init": "rm -rf demo && mkdir demo && cd demo && node ../src/maml.js init",
    "test:build": "cd demo && node ../src/maml.js build"
  },
  "homepage": "https://github.com/mamljs/maml#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mamljs/maml.git"
  },
  "dependencies": {
    "commander": "^10.0.0",
    "glob": "^9.0.1",
    "js-yaml": "^4.1.0",
    "lodash": "^4.17.21",
    "nunjucks": "^3.2.3",
    "ramda": "^0.28.0"
  },
  "devDependencies": {
    "yarn-upgrade-all": "^0.7.2"
  }
}
