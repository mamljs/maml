{
  "name": "maml",
  "version": "0.8.1",
  "description": "Static website generator taking markdown and yaml as input",
  "keywords": [
    "markdown",
    "yaml",
    "website"
  ],
  "author": "Tyler Long <tyler4long@gmail.com>",
  "license": "MIT",
  "bin": {
    "maml": "src/index.ts"
  },
  "scripts": {
    "test:init": "rm -rf demo && mkdir demo && cd demo && ts-node ../src/index.ts init",
    "test:build": "cd demo && ts-node ../src/index.ts build"
  },
  "homepage": "https://github.com/mamljs/maml#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mamljs/maml.git"
  },
  "dependencies": {
    "commander": "^10.0.0",
    "glob": "^9.1.2",
    "js-yaml": "^4.1.0",
    "lodash": "^4.17.21",
    "nunjucks": "^3.2.3"
  },
  "devDependencies": {
    "@types/lodash": "^4.14.191",
    "@types/node": "^18.14.2",
    "gts": "^3.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.5",
    "yarn-upgrade-all": "^0.7.2"
  }
}
